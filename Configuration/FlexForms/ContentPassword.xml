<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta type="array">
		<langDisable>1</langDisable>
		<langChildren>0</langChildren>
	</meta>
	<sheets>
		<general>
			<ROOT type="array">
				<TCEforms>
					<sheetTitle>Einstellungen</sheetTitle>
				</TCEforms>
				<el type="array">
					<password>
						<TCEforms type="array">
							<label>Passwort</label>
							<config type="array">
								<type>input</type>
								<size>20</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</password>
					<protection_until>
						<TCEforms type="array">
							<label>Freigabe der Inhalte ab</label>
							<config type="array">
								<type>input</type>
								<size>20</size>
								<eval>datetime</eval>
								<renderType>inputDateTime</renderType>
							</config>
						</TCEforms>
					</protection_until>
					<fallback_text>
						<TCEforms type="array">
							<config type="array">
								<type>text</type>
								<cols>48</cols>
								<rows>5</rows>
								<eval>trim</eval>
								<enableRichtext>1</enableRichtext>
								<richtextConfiguration>default</richtextConfiguration>
							</config>
							<label>Hinweistext vor Passwortfeld</label>
						</TCEforms>
					</fallback_text>
				</el>
			</ROOT>
		</general>
	</sheets>
</T3DataStructure>
